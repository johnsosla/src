    <style>
        .infoTable {
            background: #f5f5f5;
            border-collapse: separate;
            font-size: 12px;
            line-height: 24px;
            text-align: center;
            margin:5px 14px 0 14px;
        }   
        .infoTable table{
            width:100%;
        }
        .infoTable th {
            background-color: #a6afa2 !important;
            color: #fff;
            font-weight: bold;
            padding: 3px 9px;
            position: relative;
            border:none !important;
            align: center;
            text-align: center;
        }
        .infoTable th:first-child{
            border-right:1px solid #fff !important;
        }

        .infoTable th:after {
            content: '';
            display: block;
            height: 25%;
            left: 0;
            margin: 1px 0 0 0;
            position: absolute;
            top: 25%;
            width: 100%;
        }
        .infoTable td {
            border-right: 1px solid #fff;
            border-left: 1px solid #e8e8e8;
            border-top: 1px solid #fff;
            border-bottom: 1px solid #e8e8e8;
            padding: 3px 9px;
            position: relative;
            transition: all 300ms;
            min-width: 90px;
        }

        .recordTypeTable{
            border-collapse:collapse;           
        }

        .recordTypeTable table{
            width:100%;
        }
        
        .recordTypeTable th, .recordTypeTable td {
          padding: 4px 2px 4px 5px;
          border: 1px solid #dbdbdb;;
          border-collapse: collapse;
        }
        
        .recordTypeTable tr {
          border: 1px solid #dbdbdb;;
          border-collapse: collapse;
        }
        
        .recordTypeTable td {
            background-color: white;
            min-width: 90px;            
        }
           
        .sfLabel{
            font-weight:bold; font-size: 95%; float: right; color: #4d4d4d
        }  
        .customInfoText{
            /*padding: 7px;
            border: 1px solid #85B3CE;
            min-height: 30px;
            display: block;
            width: auto;
            margin: 0 0 5px 0;
