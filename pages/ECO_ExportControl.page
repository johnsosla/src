    <script type="text/javascript" >
        $j = jQuery.noConflict();
        $(document).ready(function () {
            
            $('#spinnerModal').modal({
                show: false
            });
        });
        
        function toggleShow (thisElement) 
        {
            if (thisElement.value == 'Yes' || thisElement.value == 'True' || thisElement.value == 'Unknown') 
            {
                
                $('#spinnerModal').modal({
                    show: true
                });
            }
            else
            {
                $('#spinnerModal').modal({
                    show: false
                });
                
            }
        }
        
        function showOrHideThirdPartyDescription(){ 
            $j('input:radio[name$=showThirdPartyDescription]').each(function(index){
                if($j(this).prop('checked') && $j(this).val() == 'Yes'){
                    $j('[id$=thirdPartyDescription]').show();
                    $j('[id$=thirdPartyText]').prop('required',true);
                }
                else if(!$j(this).prop('checked') && $j(this).val() == 'Yes'){
                    $j('[id$=thirdPartyDescription]').hide();
                    $j('[id$=thirdPartyText]').val('');
                }
            });
            $j('input:radio[name$=partnerSC]').each(function(index){
                if($j(this).prop('checked') && $j(this).val() == 'Yes'){
                    $j('[id$=partnerAddress]').show();
                    $j('[id$=partnerText1]').prop('required',true);
                    $j('[id$=partnerGoods]').show();
                    $j('[id$=partnerText2]').prop('required',true);
                }
                else if(!$j(this).prop('checked') && $j(this).val() == 'Yes'){
                    $j('[id$=partnerAddress]').hide();
                    $j('[id$=partnerText1]').val('');
                    $j('[id$=partnerGoods]').hide();
                    $j('[id$=partnerText2]').val('');
                }
            });
            $j('input:radio[name$=jointVenture]').each(function(index){
                //alert($j(this).val());
                if($j(this).prop('checked') && $j(this).val() == 'Yes'){
                    $j('[id$=jvName]').show();
                    $j('[id$=jvText]').prop('required',true);
                }
                else if(!$j(this).prop('checked') && $j(this).val() == 'Yes'){
                    $j('[id$=jvName]').hide();
                    $j('[id$=jvText]').val('');
                }
            });
            $j('input:radio[name$=travel]').each(function(index){
                if($j(this).prop('checked') && $j(this).val() == 'Yes'){
                    $j('[id$=travel1]').show();
                    $j('[id$=travelText1]').prop('required',true);
                    $j('[id$=travel2]').show();
                    $j('[id$=travelText2]').prop('required',true);
                }
                else if(!$j(this).prop('checked') && $j(this).val() == 'Yes'){
                    $j('[id$=travel1]').hide();
                    $j('[id$=travelText1]').val('');
                    $j('[id$=travel2]').hide();
                    $j('[id$=travelText2]').val('');
                }
            });
            if($j('[id$=governing]').val() == 'Other'){
                $j('[id$=governingCountry]').show();
                $j('[id$=governingText]').prop('required',true);
            }
            else{
                $j('[id$=governingCountry]').hide();
                $j('[id$=governingText]').val('');
            }
        }
        
        
        function showModal () {
            $('#spinnerModal').modal({
                show: true
            });
        }
        
        function hideModal () {
            $('#spinnerModal').modal('hide');
        }
        
    </script>
    <nttdcore:DLabel name="DL1" default="Export Control(s)" labelParent="{!this}" /><nttdcore:DLabel name="DL2" default="Export/Import Analysis Form" labelParent="{!this}" /><apex:sectionHeader title="{!dLabel['DL1']}" subtitle="{!dLabel['DL2']}" />
